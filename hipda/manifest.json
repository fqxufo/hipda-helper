{
  "name": "hipda helper",
  "version": "0.5",
  "description": "hipda助手扩展",
  "permissions": ["activeTab", "declarativeContent", "storage"],
  
  "background": {
    "scripts": ["jquery3.js","gbk.js","background.js"]
  },
  "omnibox": { "keyword" : "hi" },
  
  "icons": {
    "16": "images/16x.png",
    "32": "images/32x.png",
    "48": "images/48x.png",
    "128": "images/128x.png"
  },
  "manifest_version": 2,

"content_scripts": [
  {
    "matches": ["https://www.hi-pda.com/forum/*"],
    "js": ["jquery3.js","gbk.js","contentScript.js"],
    "run_at": "document_start"
  }
]
}