{
  "name": "hipda helper",
  "version": "0.672",
  "description": "hipda助手扩展",
  "permissions": ["activeTab", "declarativeContent", "storage" ],
  
  "background": {
    "scripts": ["jquery3.js","gbk.js","background.js"]
  },
  "omnibox": { "keyword" : "hi" },
  
  "icons": {
    "16": "images/16x.png",
    "32": "images/32x.png",
    "48": "images/48x.png",
    "128": "images/128x.png"
  },
  "manifest_version": 2,

    "browser_action":
    {
        "default_title": "hipda助手插件",
        "default_popup": "popup/popup.html"
    },

    "options_ui": {
      "page": "popup/popup.html",
      "open_in_tab": false
    },

"content_scripts": [
  {
    "matches": ["https://www.hi-pda.com/forum/*"],
    "js": ["jquery3.js","gbk.js","contentscript.js","topcolor.js"],
    "run_at": "document_start"
  }
],

"web_accessible_resources":[
  "clouddemo.html",
  "main.js"
],

"content_security_policy": "script-src 'self' https://cdn.apple-cloudkit.com; object-src 'self'"


}