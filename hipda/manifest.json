{
  "name": "hipda helper",
  "version": "1.0",
  "description": "Build an Extension!",
  "permissions": ["activeTab", "declarativeContent", "storage"],
  
  "background": {
    "scripts": ["gbk.js","background.js"],
    "persistent": false
  },
  "omnibox": { "keyword" : "hi" },
  
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2,

"content_scripts": [
  {
    "matches": ["https://www.hi-pda.com/forum/search.php*"],
    "js": ["jquery3.js","gbk.js","contentScript.js"],
    "run_at": "document_start"
  }
]
}